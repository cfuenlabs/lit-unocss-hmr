{
  "compilerOptions": {
    "target": "ES2020",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "unocss/vite": ["node_modules/unocss/vite"],
      "@unocss/preset-icons": ["node_modules/@unocss/preset-icons"],
      "@unocss/preset-uno": ["node_modules/@unocss/preset-uno"],
      "@unocss/preset-typography": ["node_modules/@unocss/preset-typography"],
      "@rollup/plugin-node-resolve": ["node_modules/@rollup/plugin-node-resolve"]
    },
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src", "preview.vite.config.ts", "src/pages/pages.vite.config.ts", "src/components/components.vite.config.ts"],
  "exclude": ["node_modules", ".vscode"]
}